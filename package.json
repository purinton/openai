{
    "name": "@purinton/openai",
    "type": "module",
    "version": "1.0.0",
    "description": "A simple OpenAI API client wrapper for Node.js, with ESM and TypeScript support.",
    "module": "index.mjs",
    "types": "index.d.ts",
    "files": [
        "index.mjs",
        "index.d.ts",
        "README.md",
        "LICENSE"
    ],
    "exports": {
        ".": {
            "import": "./index.mjs",
            "types": "./index.d.ts"
        }
    },
    "scripts": {
        "test": "npx --node-options=\"--experimental-vm-modules --no-warnings\" jest --detectOpenHandles --silent"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/purinton/openai.git"
    },
    "keywords": [
        "openai",
        "nodejs",
        "api",
        "client",
        "typescript",
        "esm",
        "ai",
        "purinton"
    ],
    "author": "Russell Purinton <russell@purinton.us>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/purinton/openai/issues"
    },
    "homepage": "https://github.com/purinton/openai#readme",
    "devDependencies": {
        "jest": "^30.0.1"
    },
    "jest": {
        "testEnvironment": "node",
        "testMatch": [
            "**/?(*.)+(spec|test).[jt]s?(x)",
            "**/?(*.)+(spec|test).mjs",
            "**/?(*.)+(spec|test).cjs"
        ]
    },
    "dependencies": {
        "openai": "^5.5.1"
    }
}